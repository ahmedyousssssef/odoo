#. module: crm_voip
#: model:mail.template,body_html:crm_voip.email_template_call_client_notAvailable
msgid ""
"\n"
"                <div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \">\n"
"\n"
"                    <p>Hello ${object.name},</p>\n"
"\n"
"                    <p>I just tried to call you.\n"
"                        Can you please give me your availability in order to schedule a new call?</p>\n"
"                    <p>Yours Sincerly</p>\n"
"                    <p>${user.name}</p>\n"
"\n"
"                </div>\n"
"            "
msgstr "\n                <div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \">\n\n                    <p>您好 ${object.name},</p>\n\n                    <p>我想给您打一个电话。\n                        能否告知您的方便时间以便我们给您安排一个新的电话访问?</p>\n                    <p>诚挚的问候！</p>\n                    <p>${user.name}</p>\n\n                </div>\n            "

#. module: crm_voip
#: model:mail.template,body_html:crm_voip.email_template_call_interrupted
msgid ""
"\n"
"                <div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \">\n"
"                    <p>Hello ${object.name},</p>\n"
"\n"
"                    <p>It seems that we have been interrupted during our conversation. Can you please give me your availability in order to schedule \n"
"                        a new call? </p>\n"
"                    <p>Yours Sincerly</p>\n"
"                    <p>${user.name}</p>\n"
"\n"
"                </div>\n"
"            "
msgstr "\n                <div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \">\n                    <p>您好 ${object.name},</p>\n\n                    <p>看来，我们有的通话过程被意外中断。能否请您告诉您的方便时间以便我们给您安排一个新的电话访问？ </p>\n                    <p>诚挚的问候！</p>\n                    <p>${user.name}</p>\n\n                </div>\n            "

