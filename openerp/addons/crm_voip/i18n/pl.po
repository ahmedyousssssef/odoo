#. module: crm_voip
#: model:mail.template,body_html:crm_voip.email_template_call_client_notAvailable
msgid ""
"\n"
"                <div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \">\n"
"\n"
"                    <p>Hello ${object.name},</p>\n"
"\n"
"                    <p>I just tried to call you.\n"
"                        Can you please give me your availability in order to schedule a new call?</p>\n"
"                    <p>Yours Sincerly</p>\n"
"                    <p>${user.name}</p>\n"
"\n"
"                </div>\n"
"            "
msgstr "\n                <div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \">\n\n                    <p>Witaj ${object.name},</p>\n\n                    <p>Próbowałam/próbowałem skontaktować się z Tobą.\n                        Czy możesz proszę dać mi znać o swojej dostępności w celu zaplanowania nowego połączenia?</p>\n                    <p>Z poważaniem</p>\n                    <p>${user.name}</p>\n\n                </div>\n            "

#. module: crm_voip
#: model:mail.template,body_html:crm_voip.email_template_call_interrupted
msgid ""
"\n"
"                <div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \">\n"
"                    <p>Hello ${object.name},</p>\n"
"\n"
"                    <p>It seems that we have been interrupted during our conversation. Can you please give me your availability in order to schedule \n"
"                        a new call? </p>\n"
"                    <p>Yours Sincerly</p>\n"
"                    <p>${user.name}</p>\n"
"\n"
"                </div>\n"
"            "
msgstr "\n                <div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \">\n                    <p>Witaj ${object.name},</p>\n\n                    <p>Wydaje się, że zostaliśmy rozłączeni w trakcie naszej rozmowy. Czy możesz proszę podać mi swoją dostępność w celu zaplanowania \n                        nowego połączenia? </p>\n                    <p>Z poważaniem</p>\n                    <p>${user.name}</p>\n\n                </div>\n            "

