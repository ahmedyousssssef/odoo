<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <!-- Tree View -->
        <record id="real_commission_conf_tree_view" model="ir.ui.view">
            <field name="name">commission.conf.tree.view</field>
            <field name="model">commission.configuration</field>
            <field name="arch" type="xml">
                <tree string="Commission Configuration" editable="top">
                    <field name="sales_type"/>
                    <field name="commission_type"/>
                    <field name="pay_type"/>
                    <field name="amount_percentage"/>
                    <field name="price_calculation"/>
                    <field name="commission_account_id"/>
                </tree>
            </field>
        </record>

        <!-- Action View -->
        <record id="real_commission_conf_action" model="ir.actions.act_window">
            <field name="name">Commission Configuration</field>
            <field name="res_model">commission.configuration</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree</field>
        </record>

        <!-- Menu Item -->


    </data>
</openerp>